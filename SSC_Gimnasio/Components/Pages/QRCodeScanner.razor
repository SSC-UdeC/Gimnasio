@page "/qrscanner"
@inject IJSRuntime JSRuntime

<h3>QR Code Scanner</h3>
<button @onclick="StartScanning">Start QR Scan</button>

<video id="video" autoplay playsinline style="width: 300px; height: 300px;"></video>

@code {
    private async Task StartScanning()
    {
        await JSRuntime.InvokeVoidAsync("startQRScanner", DotNetObjectReference.Create(this));
    }
}
